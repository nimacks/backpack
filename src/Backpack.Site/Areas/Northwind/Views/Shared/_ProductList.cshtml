@model IEnumerable<Backpack.Site.Models.ProductModel>
    
<div class="col-md-9">
    <table class="table table-striped table-hover ">
        <thead>
            <tr>
                <th>Product</th>
                <th>Price</th>
                <th>Available</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var product in Model)
            {
                <tr>
                    <td>@product.ProductName</td>
                    <td>@product.Price</td>
                    <td>@product.Available</td>
                    <td><button>Buy</button></td>
                </tr>
            }


        </tbody>
    </table>

</div>

